qdbus org.kde.plasmashell /PlasmaShell org.kde.PlasmaShell.evaluateScript 'loadTemplate("org.openmandriva.plasma.desktop.globalMenuPanel")'
qdbus org.kde.plasmashell /PlasmaShell org.kde.PlasmaShell.evaluateScript 'theme="McMojave"'
kwriteconfig5 --file Kvantum/kvantum.kvconfig --group General --key theme KvMojave
kwriteconfig5 --group KDE --key SingleClick false
kwriteconfig5 --group KDE --key buttonOrder mac
kwriteconfig5 --file kwinrc --group Windows --key Placement Smart
kwriteconfig5 --file plasmarc --group Theme --key name McMojave
lookandfeeltool -a com.github.vinceliuice.McMojave
kwriteconfig5 --file kcminputrc --group Mouse --key cursorTheme McMojave-circle
latte-dock&
